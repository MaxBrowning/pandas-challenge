    "# Create data frame sorted by bins\n",
    "purchase_data[\"Age Group\"] = pd.cut(purchase_data[\"Age\"], bins, labels=bin_labels, include_lowest=True)\n",
    "\n",
    "# Find total count of each age group\n",
    "dupe_age_group_count = purchase_data.groupby([\"Age Group\"]).Price.count()\n",
    "dedupe_age_group_count = deduped_SN_df.groupby([\"Age Group\"]).Price.count()\n",
    "\n",
    "# Find total amount spent per age group\n",
    "total_spend_age_group = purchase_data.groupby([\"Age Group\"]).Price.sum()\n",
    "\n",
    "# Find average purchase price per age group\n",
    "average_cost_age_group = purchase_data.groupby([\"Age Group\"]).Price.mean()\n",
    "\n",
    "# Find average spend per person per age group\n",
    "average_spend_age_group = [value / value2 for value, value2 in zip(total_spend_age_group, dedupe_age_group_count)]\n",
    "average_spend_age_group\n",
    "\n",
    "# Create the summary data frame\n",
    "age_group_purchasing_df = pd.DataFrame({\n",
    "    \"Age Ranges\": [group for group in bin_labels],\n",
    "    \"Purchase Count\": [person for person in dupe_age_group_count],\n",
    "    \"Average Purchase Price\": [avg_cost for avg_cost in average_cost_age_group],\n",
    "    \"Total Purchase Value\":[avg_purchase for avg_purchase in total_spend_age_group],\n",
    "    \"Avg Total Purchase per Person\": [avg_spend for avg_spend in average_spend_age_group]\n",
