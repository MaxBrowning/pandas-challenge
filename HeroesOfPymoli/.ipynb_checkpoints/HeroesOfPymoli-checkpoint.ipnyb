    "# Find total count of each gender\n",
    "dupe_gender_count = purchase_data.groupby([\"Gender\"]).Price.count()\n",
    "\n",
    "# Find total amount spent per gender\n",
    "total_spend_gender = purchase_data.groupby([\"Gender\"]).Price.sum()\n",
    "\n",
    "# Find average purchase price per gender\n",
    "average_cost_gender = purchase_data.groupby([\"Gender\"]).Price.mean()\n",
    "\n",
    "# Find average spend per person per gender\n",
    "average_spend_gender = [value / value2 for value, value2 in zip(total_spend_gender, dedupe_gender_count)]\n",
    "average_spend_gender\n",
    "\n",
    "# Create the summary data frame\n",
    "gender_group_df = pd.DataFrame({\n",
    "    \"Gender\": [gender for gender in gender_list],\n",
    "    \"Purchase Count\": [person for person in dupe_gender_count],\n",
    "    \"Average Purchase Price\": [avg_cost for avg_cost in average_cost_gender],\n",
    "    \"Total Purchase Value\":[avg_purchase for avg_purchase in total_spend_gender],\n",
    "    \"Avg Total Purchase per Person\": [avg_spend for avg_spend in average_spend_gender]\n",
    "})\n",
    "\n",
    "# Clean the formatting\n",
    "gender_group_df[\"Average Purchase Price\"] = gender_group_df[\"Average Purchase Price\"].map(\"${:,.2f}\".format)\n",
    "gender_group_df[\"Total Purchase Value\"] = gender_group_df[\"Total Purchase Value\"].map(\"${:,.2f}\".format)\n",
    "gender_group_df[\"Avg Total Purchase per Person\"] = gender_group_df[\"Avg Total Purchase per Person\"].map(\"${:,.2f}\".format)\n",
    "\n",
    "# Display data frame\n",
    "gender_group_df"
   ]
