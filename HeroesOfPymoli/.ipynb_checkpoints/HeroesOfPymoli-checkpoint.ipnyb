    "# Store the variables\n",
    "num_unique_items = purchase_data[\"Item Name\"].nunique()\n",
    "average_price = purchase_data[\"Price\"].mean()\n",
    "total_purchases = purchase_data[\"Price\"].count()\n",
    "total_revenue = purchase_data[\"Price\"].sum()\n",
    "\n",
    "# Create the summary data frame\n",
    "purchasing_summary_df = pd.DataFrame({\n",
    "    \"Number of Unique Items\": [num_unique_items],\n",
    "    \"Average Purchase Price\":[average_price],\n",
    "    \"Total Number of Purchases\": [total_purchases],\n",
    "    \"Total Revenue\": [total_revenue]\n",
    "})\n",
    "\n",
    "# Clean the formatting\n",
    "purchasing_summary_df[\"Average Purchase Price\"] = purchasing_summary_df[\"Average Purchase Price\"].map(\"${:.2f}\".format)\n",
    "purchasing_summary_df[\"Total Revenue\"] = purchasing_summary_df[\"Total Revenue\"].map(\"${:,.2f}\".format)\n",
    "\n",
    "# Display summary data frame\n",
    "purchasing_summary_df"
