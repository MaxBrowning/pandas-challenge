    "# Create DataFrame with each player listed only once\n",
    "deduped_SN_df = purchase_data.drop_duplicates(subset=[\"SN\"])\n",
    "\n",
    "# Create list of all gender options\n",
    "gender_list = purchase_data[\"Gender\"].unique()\n",
    "gender_list = sorted(gender_list)\n",
    "\n",
    "# Find counts for each gender (unduplicated)\n",
    "dedupe_gender_count = deduped_SN_df.groupby([\"Gender\"]).Price.count()\n",
    "\n",
    "# Find percentage for each gender\n",
    "percent_gender = [value / total_players * 100 for value in dedupe_gender_count]\n",
    "\n",
    "# Create and display summary data frame\n",
    "gender_summary_df = pd.DataFrame({\n",
    "    \"Gender\": [gender for gender in gender_list],\n",
    "    \"Total Counts\": [person for person in dedupe_gender_count],\n",
    "    \"Percentage of Players\": [round(num, 2) for num in percent_gender]\n",
    "})\n",
    "\n",
    "# Clean the formatting\n",
    "gender_summary_df[\"Percentage of Players\"] = gender_summary_df[\"Percentage of Players\"].map(\"{:,.2f}%\".format)\n",
    "\n",
    "# Display data frame\n",
    "gender_summary_df"
   ]
