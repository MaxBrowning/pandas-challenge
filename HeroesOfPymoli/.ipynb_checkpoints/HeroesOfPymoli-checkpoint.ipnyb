   "source": [
    "# Retrieve necessary columns\n",
    "popular_item_df = purchase_data[[\"Item ID\", \"Item Name\", \"Price\"]]\n",
    "\n",
    "# Add columns for purchase count and total purchase value\n",
    "popular_item_df[\"Purchase Count\"] = popular_item_df[\"Price\"].groupby(popular_item_df[\"Item ID\"]).transform(\"count\")\n",
    "popular_item_df[\"Total Purchase Value\"] = popular_item_df[\"Price\"].groupby(popular_item_df[\"Item ID\"]).transform(\"sum\")\n",
    "\n",
    "# Sort the Data Frame\n",
    "popular_item_df = popular_item_df.sort_values(\"Total Purchase Value\", ascending=False)\n",
    "\n",
    "# Remove the duplicates\n",
    "popular_item_df = popular_item_df.drop_duplicates(subset=[\"Item Name\"]).reset_index()\n",
    "\n",
    "# Clean the formatting and reorder the columns\n",
    "popular_item_df[\"Price\"] = popular_item_df[\"Price\"].map(\"${:,.2f}\".format)\n",
    "popular_item_df[\"Total Purchase Value\"] = popular_item_df[\"Total Purchase Value\"].map(\"${:,.2f}\".format)\n",
    "popular_item_df = popular_item_df[[\"Item ID\", \"Item Name\", \"Purchase Count\", \"Price\", \"Total Purchase Value\"]]\n",
    "\n",
    "# Display data frame\n",
    "popular_item_df.head()"
   ]
